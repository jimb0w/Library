	doi = {https://doi.org/10.1016/j.atherosclerosis.2003.11.002},
	isbn = {0021-9150},
	journal = {Atherosclerosis},
	keywords = {Lipoprotein metabolism; Low-density lipoprotein; High-density lipoprotein; Newborn; Mother; Pregnancy; Gene interaction; Placenta},
	number = {2},
	pages = {287--298},
	title = {Lipoprotein concentrations in newborns are associated with allelic variations in their mothers},
	url = {https://www.sciencedirect.com/science/article/pii/S0021915003004799},
